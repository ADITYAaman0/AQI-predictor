{
  "dashboard": {
    "title": "AQI Predictor - Data Pipeline",
    "tags": ["aqi", "data", "pipeline"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Data Ingestion Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(aqi_data_ingestion_total[5m])",
            "legendFormat": "{{source}} - {{data_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Records/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Data Ingestion Errors",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(aqi_data_ingestion_errors_total[5m])",
            "legendFormat": "{{source}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Data Quality Issues",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(aqi_data_quality_issues_total[5m])",
            "legendFormat": "{{issue_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Issues/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Data Processing Duration",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(aqi_data_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "{{source}} - {{operation}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Celery Tasks Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(aqi_celery_tasks_total[5m])",
            "legendFormat": "{{task_name}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Tasks/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Celery Task Duration (p95)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(aqi_celery_task_duration_seconds_bucket[5m]))",
            "legendFormat": "{{task_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Active Celery Tasks",
        "type": "graph",
        "gridPos": {"x": 0, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "aqi_celery_tasks_in_progress",
            "legendFormat": "{{task_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Active Tasks"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Total Data Ingested (24h)",
        "type": "singlestat",
        "gridPos": {"x": 12, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(increase(aqi_data_ingestion_total[24h]))",
            "refId": "A"
          }
        ],
        "format": "short",
        "valueName": "current"
      }
    ]
  }
}
