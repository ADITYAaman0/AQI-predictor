{
  "dashboard": {
    "title": "AQI Predictor - System Overview",
    "tags": ["aqi", "system", "overview"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(aqi_http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "HTTP Request Duration (p95)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(aqi_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "System CPU Usage",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "aqi_system_cpu_percent",
            "legendFormat": "CPU %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "CPU", "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "System Memory Usage",
        "type": "graph",
        "gridPos": {"x": 8, "y": 8, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "aqi_system_memory_percent",
            "legendFormat": "Memory %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Memory", "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "System Disk Usage",
        "type": "graph",
        "gridPos": {"x": 16, "y": 8, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "aqi_system_disk_usage_percent",
            "legendFormat": "Disk %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Disk", "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(aqi_http_requests_total{status=~\"4..|5..\"}[5m])",
            "legendFormat": "{{status}} errors",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Cache Hit Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(aqi_cache_hits_total[5m]) / (rate(aqi_cache_hits_total[5m]) + rate(aqi_cache_misses_total[5m]))",
            "legendFormat": "{{cache_type}} hit rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Hit Rate", "max": 1},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Active Database Connections",
        "type": "graph",
        "gridPos": {"x": 0, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "aqi_db_connections_active",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Connections"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Application Uptime",
        "type": "singlestat",
        "gridPos": {"x": 12, "y": 24, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "aqi_app_uptime_seconds",
            "refId": "A"
          }
        ],
        "format": "s",
        "valueName": "current"
      },
      {
        "id": 10,
        "title": "Total Requests (24h)",
        "type": "singlestat",
        "gridPos": {"x": 18, "y": 24, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(increase(aqi_http_requests_total[24h]))",
            "refId": "A"
          }
        ],
        "format": "short",
        "valueName": "current"
      }
    ]
  }
}
