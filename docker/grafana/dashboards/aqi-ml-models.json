{
  "dashboard": {
    "title": "AQI Predictor - ML Models Performance",
    "tags": ["aqi", "ml", "models"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "1m",
    "panels": [
      {
        "id": 1,
        "title": "Model Prediction Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(aqi_model_predictions_total[5m])",
            "legendFormat": "{{model_name}} v{{model_version}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Predictions/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Model Prediction Duration (p95)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(aqi_model_prediction_duration_seconds_bucket[5m]))",
            "legendFormat": "{{model_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Model RMSE",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "aqi_model_rmse",
            "legendFormat": "{{model_name}} v{{model_version}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "RMSE (μg/m³)"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [25], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High Model RMSE",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 4,
        "title": "Model MAE",
        "type": "graph",
        "gridPos": {"x": 8, "y": 8, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "aqi_model_mae",
            "legendFormat": "{{model_name}} v{{model_version}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "MAE (μg/m³)"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Model Accuracy",
        "type": "graph",
        "gridPos": {"x": 16, "y": 8, "w": 8, "h": 8},
        "targets": [
          {
            "expr": "aqi_model_accuracy_percent",
            "legendFormat": "{{model_name}} v{{model_version}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Accuracy", "max": 100},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [70], "type": "lt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "Low Model Accuracy",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 6,
        "title": "Prediction Errors",
        "type": "graph",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(aqi_prediction_errors_total[5m])",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Total Predictions by Model",
        "type": "piechart",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (model_name) (aqi_model_predictions_total)",
            "legendFormat": "{{model_name}}",
            "refId": "A"
          }
        ]
      }
    ]
  }
}
